<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>vHELP-JITDispatch</display-name>
  <servlet>
    <description></description>
    <display-name>LoginAction</display-name>
    <servlet-name>LoginAction</servlet-name>
    <servlet-class>com.verizon.JITDispatch.login.action.LoginAction</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>LogoutAction</display-name>
    <servlet-name>LogoutAction</servlet-name>
    <servlet-class>com.verizon.JITDispatch.login.action.LogoutAction</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>CreateRequestAction</display-name>
    <servlet-name>CreateRequestAction</servlet-name>
    <servlet-class>com.verizon.JITDispatch.request.action.CreateRequestAction</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>TechAction</display-name>
    <servlet-name>TechAction</servlet-name>
    <servlet-class>com.verizon.JITDispatch.tech.action.TechAction</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>CancelRequestAction</display-name>
    <servlet-name>CancelRequestAction</servlet-name>
    <servlet-class>com.verizon.JITDispatch.request.action.CancelRequestAction</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>RescheduleRequestAction</display-name>
    <servlet-name>RescheduleRequestAction</servlet-name>
    <servlet-class>com.verizon.JITDispatch.request.action.RescheduleRequestAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginAction</servlet-name>
    <url-pattern>/LoginAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutAction</servlet-name>
    <url-pattern>/LogoutAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CreateRequestAction</servlet-name>
    <url-pattern>/CreateRequestAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TechAction</servlet-name>
    <url-pattern>/TechAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CancelRequestAction</servlet-name>
    <url-pattern>/CancelRequestAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RescheduleRequestAction</servlet-name>
    <url-pattern>/RescheduleRequestAction</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>/LoginAction</welcome-file>
    <welcome-file>jsp/common/home.jsp</welcome-file>
    <welcome-file>NewFile.jsp</welcome-file>
  </welcome-file-list>
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
</web-app>